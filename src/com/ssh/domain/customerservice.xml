<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ssh.domain">  
    <class name="CustomerService" table="customer_service">  
        <id name="id" column="id">  
            <generator class="native"></generator>  
        </id>  
        <property name="userId" column="user_id"></property>  
        <property name="carModelId" column="car_model_id"></property> 
        <property name="time" column="time"></property>   
        <property name="reason" column="reason"></property>
    </class>  
</hibernate-mapping>  