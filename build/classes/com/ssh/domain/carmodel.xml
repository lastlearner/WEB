<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ssh.domain">
	<class name="CarModel" table="car_model">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="modelName" column="modelName"></property>
		<property name="doorCount" column="doorCount"></property>
		<property name="seatCount" column="seatCount"></property>
		<property name="price" column="price"></property>
		<property name="engineSpeed" column="engineSpeed"></property>
		<property name="color" column="color"></property>
		<property name="storage" column="storage"></property>
	</class>
</hibernate-mapping>  