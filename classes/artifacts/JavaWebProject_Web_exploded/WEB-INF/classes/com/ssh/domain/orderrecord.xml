<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ssh.domain">
	<class name="OrderRecord" table="order_record">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="userID" column="userID"></property>
		<property name="userName" column="userName"></property>
		<property name="carID" column="carID"></property>
		<property name="carName" column="carName"></property>
		<property name="discount" column="discount"></property>
		<property name="price" column="price"></property>
		<property name="time" column="time"></property>
		<property name="note" column="note"></property>
	</class>
</hibernate-mapping>  